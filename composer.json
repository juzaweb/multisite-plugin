{
    "name": "juzaweb/multisite",
    "description": "Juzaweb CMS multisite",
    "license": "MIT",
    "homepage": "https://juzaweb.com",
    "require": {
        "php": "^8.0|^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "6.0 | 7.0 | 7.5 | 8.4 | ^8.5"
    },
    "autoload": {
        "psr-4": {
            "Juzaweb\\Multisite\\Tests\\": "tests/",
            "Juzaweb\\Multisite\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Juzaweb\\Multisite\\Providers\\MultisiteServiceProvider"
            ]
        }
    }
}
